#!/usr/bin/awk -f

$1 < 0.000000000000000000001		{ printf $1*1000000000000000000000000"y"	; next }
$1 < 0.000000000000000001		{ printf $1*1000000000000000000000"z"		; next }
$1 < 0.000000000000001			{ printf $1*1000000000000000000"a"		; next }
$1 < 0.000000000001			{ printf $1*1000000000000000"f"			; next }
$1 < 0.000000001			{ printf $1*1000000000000"p"			; next }
$1 < 0.000001				{ printf $1*1000000000"n"			; next }
$1 < 0.001				{ printf $1*1000000"Âµ"				; next }
$1 < 0.01				{ printf $1*1000"m"				; next }
$1 < 0.1				{ printf $1*100"c"				; next }
$1 < 1					{ printf $1*10"d"				; next }
$1 < 10					{ printf $1					; next }
$1 < 100				{ printf $1/10"da"				; next }
$1 < 1000				{ printf $1/100"h"				; next }
$1 < 1000000				{ printf $1/1000"k"				; next }
$1 < 1000000000				{ printf $1/1000000"M"				; next }
$1 < 1000000000000			{ printf $1/1000000000"G"			; next }
$1 < 1000000000000000			{ printf $1/1000000000000"T"			; next }
$1 < 1000000000000000000		{ printf $1/1000000000000000"P"			; next }
$1 < 1000000000000000000000		{ printf $1/1000000000000000000"E"		; next }
$1 < 1000000000000000000000000		{ printf $1/1000000000000000000000"Z"		; next }
$1 > 1000000000000000000000000		{ printf $1/1000000000000000000000000"Y"	; next }
